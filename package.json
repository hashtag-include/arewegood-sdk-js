{
  "name": "arewegood",
  "version": "0.1.0",
  "description": "SDK for arewegood - monitoring for apps and services that matter",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "backoff": "^2.4.1",
    "bunyan": "^1.3.5",
    "hulksmash": "0.0.3",
    "promise": "^6.1.0",
    "request": "^2.55.0"
  },
  "devDependencies": {
    "body-parser": "^1.12.4",
    "express": "^4.12.4",
    "express-bunyan-logger": "^1.1.1",
    "mocha": "^2.1.0",
    "passport": "^0.2.1",
    "passport-http-bearer": "^1.0.1",
    "blanket": "^1.1.7",
    "coveralls": "^2.11.2",
    "mocha-lcov-reporter": "0.0.2"
  },
  "scripts": {
    "test": "mocha --recursive --require blanket --reporter mocha-lcov-reporter | node node_modules/coveralls/bin/coveralls.js & mocha --recursive"
  },
  "config": {
      "blanket": {
        "pattern": ["index.js","lib","lib/connections"],
        "data-cover-never": ["node_modules", "test"]
      }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hashtag-include/arewegood-sdk-js.git"
  },
  "keywords": [
    "monitor",
    "error",
    "exception"
  ],
  "author": "Ben Greenier",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hashtag-include/arewegood-sdk-js/issues"
  },
  "homepage": "https://github.com/hashtag-include/arewegood-sdk-js"
}
